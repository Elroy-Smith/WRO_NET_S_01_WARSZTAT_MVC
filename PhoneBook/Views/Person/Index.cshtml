@model List<PhoneBook.Models.PersonModel>

<table border="1">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Created</th>
        <th>Updated</th>
    </tr>

    @foreach (var person in Model)
    {
        <tr>
            <td>@person.ID</td>
            <td>@person.FirstName</td>
            <td>@person.LastName</td>
            <td>@person.Phone</td>
            <td>@person.Email</td>
            <td>@person.Created</td>
            <td>@person.Updated</td>
        </tr>
    }
</table>